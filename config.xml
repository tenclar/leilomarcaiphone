<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns       = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "br.com.leilomarca.phonegapapp"
        versionCode = ""
        version     = "1.0" >

	<name>Leilo Marca</name>

	<description>Leil&#245;es Rurais da Regi&#227;o Norte</description>

	<author href="http://www.leilomarca.com.br" email="contato@leilomarca.com.br">Ennyelson de Souza</author>

	<gap:platform name="ios" />
	
	<!-- General preferences -->
	<preference name="permissions" value="none"/>

	<preference name="DisallowOverscroll" value="true" />
	<preference name="webviewbounce" value="false" />

	<!-- PhoneGap plugin declaration -->
	<plugin name="cordova-plugin-inappbrowser" source="npm" />
	<plugin name="cordova-plugin-network-information" source="npm" />
	<plugin name="cordova-plugin-splashscreen" source="npm" />
	<plugin name="cordova-plugin-device" source="npm" />
	<plugin name="cordova-plugin-statusbar" source="npm" />
	<plugin name="cordova-plugin-customurlscheme" spec="4.2.0" >
		<param name="URL_SCHEME" value="appleilomarca" />
	</plugin>
	<plugin name="pushwoosh-pgb-plugin" spec="6.5.3" source="npm" />

	<!-- SplashScreen configuration -->
	<preference name="AutoHideSplashScreen" value="false" />
	<preference name="FadeSplashScreenDuration" value="1000" />
	<preference name="ShowSplashScreenSpinner" value="false" />
	<gap:config-file platform="ios" parent="UIStatusBarHidden"><true/></gap:config-file>
	<gap:config-file platform="ios" parent="UIViewControllerBasedStatusBarAppearance"><false/></gap:config-file>

	<!-- Icons and Splashscreens declaration -->

	<icon src="themes/wpak-theme-bootstrap-master/res/icons/ios/icon-60@3x.png" gap:platform="ios" width="180" height="180"/>
	<icon src="themes/wpak-theme-bootstrap-master/res/icons/ios/icon-60@2x.png" gap:platform="ios" width="120" height="120"/>
	<icon src="themes/wpak-theme-bootstrap-master/res/icons/ios/icon@2x.png" gap:platform="ios" width="114" height="114"/>
	<icon src="themes/wpak-theme-bootstrap-master/res/icons/ios/icon-40@2x.png" gap:platform="ios" width="80" height="80"/>
	<icon src="themes/wpak-theme-bootstrap-master/res/icons/ios/icon-76.png" gap:platform="ios" width="76" height="76"/>
	<icon src="themes/wpak-theme-bootstrap-master/res/icons/ios/icon-76@2x.png" gap:platform="ios" width="152" height="152"/>
	

	<icon src="themes/wpak-theme-bootstrap-master/res/icons/ios/icon-60.png" gap:platform="ios" width="60" height="60"/>
	<icon src="themes/wpak-theme-bootstrap-master/res/icons/ios/icon-small@2x.png" gap:platform="ios" width="58" height="58"/>
	<icon src="themes/wpak-theme-bootstrap-master/res/icons/ios/icon.png" gap:platform="ios" width="57" height="57"/>
	<icon src="themes/wpak-theme-bootstrap-master/res/icons/ios/icon-small.png" gap:platform="ios" width="29" height="29"/>


    <gap:splash gap:platform="ios" width="2208" height="1242" src="themes/wpak-theme-bootstrap-master/res/splashscreens/ios/Default-736h-Lanscape@3x~iphone.png" />
	<gap:splash gap:platform="ios" width="1242" height="2208" src="themes/wpak-theme-bootstrap-master/res/splashscreens/ios/Default-736h-Portrait@3x~iphone.png" />	
	
	<gap:splash gap:platform="ios" width="750"  height="1334" src="themes/wpak-theme-bootstrap-master/res/splashscreens/ios/Default-667h-Portrait@2x~iphone.png" />
	<gap:splash gap:platform="ios" width="1334" height="750"  src="themes/wpak-theme-bootstrap-master/res/splashscreens/ios/Default-667h-Landscape@2x~iphone.png" />
	
	<gap:splash gap:platform="ios" width="1136" height="640"  src="themes/wpak-theme-bootstrap-master/res/splashscreens/ios/Default-568h-Landscape@2x~iphone.png" />
	<gap:splash gap:platform="ios" width="640"  height="1136" src="themes/wpak-theme-bootstrap-master/res/splashscreens/ios/Default-568h-Portrait@2x~iphone.png" />
	<gap:splash gap:platform="ios" width="960"  height="640"  src="themes/wpak-theme-bootstrap-master/res/splashscreens/ios/Default-Landscape@2x~iphone.png" />
